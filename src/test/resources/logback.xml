<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>[%level] %date{"yyyy-MM-dd'T'HH:mm:ss.SSS'Z' '(UTC+0)'", UTC} %logger:%line - %msg%n</pattern>
    </encoder>
  </appender>
  
  <root level="trace">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
